<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#8F363636" />
            <corners android:radius="6dip" />
        </shape>
    </item>
    <item
        android:top="2dip"
        android:right="2dip"
        android:bottom="2dip"
        android:left="2dip">
        <shape
            android:shape="rectangle">

            <solid
                android:color="@color/tenfour" />
            <stroke
                android:width="1dip"
                android:color="@color/tenfour" />
            <corners android:radius="4dip" />
            <size
                android:width="32dp"
                android:height="32dp" />
        </shape>
    </item>
</layer-list>